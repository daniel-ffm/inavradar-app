<?xml version='1.0' encoding='utf-8'?>
<widget id="de.iumt.inavradarapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>INAVradar</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="android-minSdkVersion" value="18" />
    <preference name="android-targetSdkVersion" value="18" />
    <preference name="WindowSize" value="400x650" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-bluetooth-serial" spec="^0.4.7" />
    <engine name="browser" spec="^5.0.4" />
    <engine name="android" spec="^6.4.0" />
    <engine name="osx" spec="^4.0.2" />
    <engine name="ios" spec="^4.5.5" />
</widget>
